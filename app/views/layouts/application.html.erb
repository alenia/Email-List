<!DOCTYPE html>
<html>
<head>
  <title>Cottage Game Night Mailing List</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
  <body>
    <% if flash[:notice] %>
      <div class="notice">
        <%= flash[:notice] %>
      </div>
      <% elsif flash[:error] %>
        <div class="error">
          <%= flash[:error] %>
        </div>
      <% end %>
    <div class="main">
      <%= yield %>
    </div>

    <ul class="nav">
      <li><%=link_to "Entry", root_path%></li>
      <li><%=link_to "Rolodex", contacts_path%></li>
    </ul>
  </body>
</html>
